{
  "name": "loopback4-notifications",
  "version": "1.3.1",
  "description": "An extension for setting up various notification mechanisms in loopback4 application, vis-a-vis, Push notification, SMS notification, Email notification",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "echo \"No test specified\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-notifications"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.5.1",
    "@loopback/context": "^3.8.1",
    "@loopback/core": "^2.7.0",
    "@loopback/rest": "^5.0.1",
    "@types/nodemailer": "^6.4.0",
    "firebase-admin": "^9.2.0",
    "nodemailer": "^6.4.13",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.1",
    "@loopback/eslint-config": "^7.0.1",
    "@loopback/testlab": "^3.1.5",
    "@types/node": "^10.17.24",
    "@types/pubnub": "^4.27.3",
    "@types/socket.io-client": "^1.4.33",
    "aws-sdk": "^2.683.0",
    "eslint": "^7.0.0",
    "pubnub": "^4.27.6",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.3"
  },
  "peerDependencies": {
    "aws-sdk": "^2.683.0",
    "pubnub": "^4.27.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
